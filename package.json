{
    "name": "video-consumer-backend",
    "version": "1.0.0",
    "description": "Backend for video consumer application",
    "main": "dist/index.js",
    "scripts": {
        "start": "node dist/index.js",
        "dev": "nodemon --exec ts-node src/index.ts",
        "build": "tsc && cp src/.env* dist/ 2>/dev/null || true && cp src/config.json dist/ 2>/dev/null || true",
        "tsc-check": "tsc --noEmit",
        "vercel-build": "echo 'Build step completed'"
    },
    "dependencies": {
        "@aws-sdk/client-ec2": "^3.767.0",
        "@aws-sdk/client-ecs": "^3.766.0",
        "@aws-sdk/client-s3": "^3.525.0",
        "@types/express-serve-static-core": "^5.0.6",
        "axios": "^1.6.7",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.2",
        "multer": "^1.4.5-lts.1",
        "serverless-http": "^3.2.0",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@types/body-parser": "^1.19.5",
        "@types/connect": "^3.4.38",
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.1",
        "@types/http-errors": "^2.0.4",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/mime": "^3.0.4",
        "@types/multer": "^1.4.12",
        "@types/node": "^22.13.11",
        "@types/qs": "^6.9.18",
        "nodemon": "^2.0.22",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.2"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "license": "MIT"
}