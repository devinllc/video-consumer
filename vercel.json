{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/health",
            "dest": "src/index.js",
            "methods": [
                "GET"
            ]
        },
        {
            "src": "/api/config",
            "dest": "src/index.js",
            "methods": [
                "GET",
                "POST"
            ]
        },
        {
            "src": "/api/upload",
            "dest": "src/index.js",
            "methods": [
                "POST"
            ]
        },
        {
            "src": "/api/check-upload-ready",
            "dest": "src/index.js",
            "methods": [
                "GET"
            ]
        },
        {
            "src": "/api/jobs",
            "dest": "src/index.js",
            "methods": [
                "GET"
            ]
        },
        {
            "src": "/api/jobs/(.*)",
            "dest": "src/index.js",
            "methods": [
                "GET"
            ]
        },
        {
            "src": "/api/test-connection",
            "dest": "src/index.js",
            "methods": [
                "GET"
            ]
        },
        {
            "src": "/api/test-s3",
            "dest": "src/index.js",
            "methods": [
                "GET"
            ]
        },
        {
            "src": "/api/start-transcoding",
            "dest": "src/index.js",
            "methods": [
                "POST"
            ]
        },
        {
            "src": "/api/(.*)",
            "dest": "src/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "frontend-app/$1"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}